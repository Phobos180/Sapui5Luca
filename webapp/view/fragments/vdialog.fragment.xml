<core:FragmentDefinition xmlns:core="sap.ui.core" 
    xmlns="sap.m" 
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" 
    xmlns:l="sap.ui.layout" 
    xmlns:ui="sap.ui.table">

    <Dialog id="modifyParametersDialog" title="Modifica Parametri" contentWidth="450px" titleAlignment="Center">
        <VBox alignItems="Center">
                <Input id="id" placeholder="Enter id" class="sapUiTinyMarginTopBottom" liveChange="onInputChange"/>
                <Input id="userId" placeholder="Enter userId" class="sapUiTinyMarginTopBottom" liveChange="onInputChange"/>
                <Input id="title" placeholder="Enter title" class="sapUiTinyMarginTopBottom" liveChange="onInputChange"/>
                <Input id="completed" placeholder="Enter completed" class="sapUiTinyMarginTopBottom" liveChange="onInputChange"/>
            <Button text="Applica Modifiche" press="onApplyChanges" class="sapUiTinyMarginTopBottom" enabled="{= ${temp>/deleteEnable}}"/>
        </VBox>
    </Dialog>

</core:FragmentDefinition>